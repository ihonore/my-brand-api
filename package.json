{
  "name": "my-brand-api",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "start": "npm run build && better-npm-run prod",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node ./src/app.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "test": {

      "command": "nyc mocha ./tests/**/*.test.js --exit --inspect=0.0.0.0:9229 --timeout 120000",
      "env": {
        "NODE_ENV": "test",
        "PORT": 5000
      }
    },
    "prod": {
      "command": "node ./dist/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CYIMANA-Faisal/my-brand-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CYIMANA-Faisal/my-brand-api/issues"
  },
  "homepage": "https://github.com/CYIMANA-Faisal/my-brand-api#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "better-npm-run": "^0.1.1",
    "cloudinary": "^1.28.1",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.8",
    "multer": "^1.4.4",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1"
  }
}
