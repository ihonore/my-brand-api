{
  "name": "my-brand-api",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "start": "npm run build && better-npm-run prod",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon ./src/app.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "test": {
      "command": "nodemon ./src/app.js",
      "env": {
        "NODE_ENV": "test",
        "PORT": 5000
      }
    },
    "prod": {
      "command": "node ./dist/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CYIMANA-Faisal/my-brand-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CYIMANA-Faisal/my-brand-api/issues"
  },
  "homepage": "https://github.com/CYIMANA-Faisal/my-brand-api#readme",
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.16.9",
    "better-npm-run": "^0.1.1",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "mongoose": "^6.1.8"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
